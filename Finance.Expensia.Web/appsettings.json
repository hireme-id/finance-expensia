{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "GoogleConfig": {
      "OAuthGoogle": {
        "ClientId": "xxxxxxxxxxxxx-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx.apps.googleusercontent.com",
        "ClientSecret": "xxxxxx-xxxxxxxxxxxxxxxxxxxxxxxxxxxx",
        "TokenUrl": "https://oauth2.googleapis.com/token"
      },
        "UserProfileUrl": "https://people.googleapis.com/v1/people/me"
    },
    "ConnectionStrings": {
        "SqlServer": "Server=localhost,1433;Database=expensia-dev;User Id=xxxxxx;Password=xxxxxx;TrustServerCertificate=True"
    },
    "SecurityConfig": {
        "MaximumLoginRetry": 5,
        "Issuer": "xxxxxx",
        "Audience": "xxxxxx",
        "SecretKey": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx",
        "TokenExpired": 60,
        "SessionExpired": 8
    },
    "StorageConfig": {
        "UploadFileLocation": "upload"
    }
}
